AS	=	MASM
LK	=	OPTLINK
ASFLAGS	=	


MMP.INC	:	MUSYA.MMP
	BIN2ASM MUSYA.MMP MMP.INC

MP2.INC	:	MUSYA.MP2
	BIN2ASM MUSYA.MP2 MP2.INC

MUSYA.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

MAINLOOP.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

MACLIB.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

CONSOLE.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

SCHIP.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

SMAPSS.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

CHIPSET.OBJ	:	$*.ASM CIP0001.INC CIP0002.INC CIP0003.INC CIP0004.INC
	$(AS) $(ASFLAGS) $*.ASM,$@;

CHARSET.OBJ	:	$*.ASM MY1.INC
	$(AS) $(ASFLAGS) $*.ASM,$@;

MOUSE.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

IOBOX.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

IOPORT.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM,$@;

MAPSET.OBJ	:	$*.ASM MMP.INC MP2.INC
	$(AS) $(ASFLAGS) $*.ASM,$@;

MUSYA.COM	:	MUSYA.OBJ CHARSET.OBJ CHIPSET.OBJ SMAPSS.OBJ \
		SCHIP.OBJ MACLIB.OBJ CONSOLE.OBJ MOUSE.OBJ IOBOX.OBJ \
		IOPORT.OBJ MUSYA.OBJ MAINLOOP.OBJ CHARSET.OBJ CHIPSET.OBJ \
		SMAPSS.OBJ SCHIP.OBJ MACLIB.OBJ IOBOX.OBJ MOUSE.OBJ IOPORT.OBJ \		IOBOX.OBJ MAPSET.OBJ
		$(LK) @LINKFILE
		COPY MUSYA.COM \WORK
