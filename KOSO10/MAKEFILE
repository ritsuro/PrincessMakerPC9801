PRAS	=	KASM
AS	=	OPTASM
ASFLAGS	=	
CPY	=	COPY
RAMDISK	=	E:\

\WORK\WINDOW1.COM	:	WINDOW1.ASM WINDOW2.ASM WINDOW3.ASM WINDOW4.ASM
	$(PRAS) WINDOW1.ASM WINDOW1.W
	$(PRAS) WINDOW2.ASM WINDOW2.W
	$(PRAS) WINDOW3.ASM WINDOW3.W
	$(PRAS) WINDOW4.ASM WINDOW4.W
	$(AS) $(ASFLAGS) WINDOW1.W,..\EXEC\WINDOW1;
	DEL WINDOW1.W
	DEL WINDOW2.W
	DEL WINDOW3.W
	DEL WINDOW4.W
	CD ..\EXEC
	OPTLINK /T WINDOW1;
	COPY WINDOW1.COM \WORK
	DEL WINDOW1.COM
	CD ..\KOSO10
